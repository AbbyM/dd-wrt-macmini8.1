		<div class="label"><% tran("service.rsync_srv"); %></div>
		<input class="spaceradio" type="radio" name="rsyncd_enable" value="1" <% nvram_checked("rsyncd_enable", "1"); %> /> <% tran("share.enable"); %>&nbsp;
